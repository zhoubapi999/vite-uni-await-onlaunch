  "keywords": [
    "vite",
    "plugin"
  ],
  "author": "Zhoubapi999",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "normalize-path": "^3.0.0",
    "prettier": "^2.6.2",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2",
    "vite": "*"
  }
}
